

Options +FollowSymLinks
RewriteEngine On

RewriteCond %{SERVER_PORT} 80
RewriteCond %{REQUEST_URI} !^/[0-9]+\..+\.cpaneldcv$
RewriteCond %{REQUEST_URI} !^/\.well-known/pki-validation/[A-F0-9]{32}\.txt(?:\ Comodo\ DCV)?$
RewriteRule ^(.*)$ https://husseinhassan.org/$1 [R,L] 

# Add the redirects for urls
RewriteRule ^/?$ index.html [NC,L]
RewriteRule ^about/?$ about.html [NC,L]
RewriteRule ^service/?$ service.html [NC,L]
RewriteRule ^blog/?$ blog.html [NC,L]
RewriteRule ^payment/?$ payment.html [NC,L]
RewriteRule ^contact/?$ contact.html [NC,L]

# php -- BEGIN cPanel-generated handler, do not edit
# Set the “ea-php73” package as the default “PHP” programming language.
<IfModule mime_module>
  AddHandler application/x-httpd-ea-php73 .php .php7 .phtml
</IfModule>
# php -- END cPanel-generated handler, do not edit
